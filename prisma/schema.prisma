generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
}

model Trade {
  id        String   @id @default(uuid())
  symbol    String
  signal    String
  entry     Float
  sl        Float
  tp        Float
  createdAt DateTime @default(now())
}
